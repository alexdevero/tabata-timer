{
  "name": "tabata-timer",
  "version": "0.0.1",
  "description": "Tabata timer app built with electron.",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdevero/tabata-timer.git"
  },
  "homepage": "https://github.com/alexdevero/tabata-timer#readme",
  "bugs": {
    "url": "https://github.com/alexdevero/tabata-timer/issues"
  },
  "author": {
    "name": "Alex Devero",
    "url": "https://alexdevero.com/"
  },
  "contributors": [
    {
      "name": "",
      "email": "",
      "url": ""
    }
  ],
  "keywords": [
    "app",
    "electron",
    "fitness",
    "open",
    "open-source",
    "source",
    "tabata",
    "timer"
  ],
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0",
    "yarn": ">=1.0"
  },
  "main": "main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production parcel build src/index.html --no-minify --public-url /",
    "package:all": "npm run build && electron-packager ./ --out=./builds --overwrite --platform=all --icon=src/assets/tabata-timer-icon",
    "package:linux": "npm run build && electron-packager ./ --out=./builds --overwrite --platform=linux --icon=src/assets/tabata-timer-icon",
    "package:macappstore": "npm run build && electron-packager ./ --out=./builds --overwrite --platform=mas --icon=src/assets/tabata-timer-icon",
    "package:osx": "npm run build && electron-packager ./ --out=./builds --overwrite --platform=darwin --icon=src/assets/tabata-timer-icon",
    "package:win": "npm run build && electron-packager ./ --out=./builds --overwrite --platform=win32 --icon=src/assets/tabata-timer-icon",
    "start": "parcel ./src/index.html -p 1337",
    "test": "echo \"No script specified\""
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-react": "^6.24.1",
    "electron": "^3.0.0-beta.1",
    "electron-packager": "^12.1.0",
    "parcel-bundler": "^1.9.4",
    "styled-components": "^3.3.3"
  }
}
