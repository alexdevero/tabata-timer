{
  "name": "tabata-timer",
  "version": "0.0.1",
  "description": "Tabata timer app built with electron.",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdevero/tabata-timer.git"
  },
  "homepage": "https://github.com/alexdevero/tabata-timer#readme",
  "bugs": {
    "url": "https://github.com/alexdevero/tabata-timer/issues"
  },
  "author": {
    "name": "Alex Devero",
    "url": "https://alexdevero.com/"
  },
  "contributors": [
    {
      "name": "",
      "email": "",
      "url": ""
    }
  ],
  "keywords": [
    "app",
    "electron",
    "fitness",
    "open",
    "open-source",
    "source",
    "tabata",
    "timer"
  ],
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0",
    "yarn": ">=1.0"
  },
  "main": "src/main.js",
  "scripts": {
    "build": "webpack --config webpack.build.config.js --mode production",
    "prod": "webpack --mode production --config=./webpack.build.config.js && electron --noDevServer .",
    "start": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds",
    "test": "echo \"No script specified\"",
    "package:beta": "cross-env NODE_ENV=prod electron-packager ./beta --out=./builds-beta --overwrite --platform=win32",
    "package:win": "cross-env NODE_ENV=prod electron-packager ./ --out=./builds --overwrite --platform=win32",
    "start:beta": "cross-env NODE_ENV=dev electron ./beta/app.js"
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron": "^3.0.0-beta.8",
    "electron-packager": "^12.1.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-nesting": "^6.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-pxtorem": "^4.0.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  }
}
